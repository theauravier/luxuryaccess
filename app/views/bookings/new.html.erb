<%= simple_form_for @booking do |f| %>
  <ul class="list-inline">
    <li><%= f.input :start_day, label: 'From', error: 'Starting day must be should be a good day' %><li>
    <li><%= f.input :end_day, label: 'Until', as: => :date_picker, error: 'Starting day must be should be a bad day' %></li>

  </ul>
  <div class="text-center">
    <%= f.button :submit, {class:"btn btn-success"} %>
  </div>

<% end %>
